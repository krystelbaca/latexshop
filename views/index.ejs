  <!-- Modal Carrito-->
  <div class="modal fade" id="carritoModal" role="dialog">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Modal Header</h4>
        </div>
        <div class="modal-body">
          <table class="table">
            <thead>
              <tr>
                <th style="text-align:center;">Producto</th>
                <th style="text-align:center;">Precio</th>
                <th style="text-align:center;">Talla</th>
                <th style="text-align:center;">Cantidad</th>
                <th style="text-align:center;">Total</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="producto in cartProducts">
                <td style="text-align:center;">{{producto.productName}}</td>
                <td style="text-align:center;">{{producto.price}}</td>
                <td style="text-align:center;">{{producto.talla}}</td>
                <td style="text-align:center;">{{producto.cantidad}}</td>
                <td style="text-align:center;">{{producto.total}}</td>
                <td style="text-align:center;"><a ng-click="removerObjeto(producto.idProducto)" class="btn btn-default"><i class="fa fa-shopping-cart"></i>Eliminar</a></td>
              </tr>
              <tr>
                <td></td>
                <td></td>
                <td></td>
                <td style="text-align:center;">Total Carrito:</td>
                <td style="text-align:center;">{{totalCarro}}</td>
              </tr>
            </tbody>
          </table>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>
</div>


<div id="loginModal" class="modal fade forget-modal" tabindex="-1" role="dialog" aria-labelledby="myForgetModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-sm">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">
					<span aria-hidden="true">×</span>
					<span class="sr-only">Close</span>
				</button>
				<h4 class="modal-title">Login</h4>
			</div>
			<div class="modal-body">
        <form role="form" action="/iniciarSesion" method="post" id="login-form" autocomplete="off">
				<p>Email</p>
				<input type="email" name="email" id="email" class="form-control" autocomplete="off">
        <p>Password</p>
				<input type="password" name="password" id="password" class="form-control" autocomplete="off">
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
				<input type="submit" id="btn-login" class="btn btn-custom btn-lg btn-block" value="Log in">
			</div>
		</div> <!-- /.modal-content -->
	</div> <!-- /.modal-dialog -->
